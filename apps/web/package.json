{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' NEXT_PRIVATE_WORKERS=2 next build",
    "build:cf": "NODE_OPTIONS=--max-old-space-size=3584 NEXT_PRIVATE_WORKERS=2 next build && NODE_OPTIONS=--max-old-space-size=3584 node scripts/build-opennext.js",
    "preview:cf": "npx opennextjs-cloudflare preview",
    "deploy:cf": "npx opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "build:webpack": "NODE_OPTIONS='--max-old-space-size=4096' NEXT_PRIVATE_WORKERS=4 next build --webpack && node scripts/build-cf.js",
    "build:simulate": "node scripts/simulate-cloudflare-build.js",
    "build:debug": "BUILD_DEBUG=true NODE_OPTIONS='--max-old-space-size=4096' NEXT_PRIVATE_WORKERS=4 next build && BUILD_DEBUG=true node scripts/build-cf.js",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrate": "wrangler d1 migrations apply omni-cms --local",
    "db:migrate:prod": "wrangler d1 migrations apply omni-cms",
    "db:seed": "tsx src/lib/db/seed.ts",
    "db:seed:demo": "tsx src/lib/db/seed-demo.ts",
    "db:seed:dev": "tsx src/lib/db/seed-dev.ts",
    "db:studio": "drizzle-kit studio",
    "fetch:coventry": "tsx src/scripts/fetch-coventry-programs.ts"
  },
